<Window x:Class="Frame_Example.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Frame_Example"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid Background="lightblue">
        <StackPanel HorizontalAlignment="Left" Width="140">
            <Button x:Name="btnMsft" Content="Microsoft" Width="100" Height="40" Margin="10"
                    Click="btnMsft_Click" />
            <Button x:Name="btnIntel" Content="Intel" Width="100" Height="40" Margin="10"
                    Click="btnIntel_Click" />
        </StackPanel>
        <Frame x:Name="frame" Margin="125,0,5,5" Visibility="Hidden"
               Source="http://www.microsoft.com" />
        <Frame x:Name="frame1" Margin="125,0,5,5" Visibility="Hidden"
               Source="http://www.intel.com" />


    </Grid>
</Window>
